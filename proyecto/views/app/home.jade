extends ../layout.jade
block contenido
	h1 Vista de Marcadores
	for imagen in imagenes
		div(class="img-container")
			h2= imagen.title
			img(src="/public/imagenes/#{imagen._id}.#{imagen.extension}",class="main-img")
			if(typeof imagen.creator != "undefined")
				p Por #{imagen.creator.email}
				p Contenido: #{imagen.contenido}
